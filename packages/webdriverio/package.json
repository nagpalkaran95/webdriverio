{
  "name": "webdriverio",
  "description": "Next-gen WebDriver test automation framework for Node.js",
  "version": "5.0.0",
  "homepage": "http://webdriver.io",
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "main": "./build/index.js",
  "bin": {
    "wdio": "./bin/wdio.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio/webdriverio.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "engines": {
    "node": ">= 4.8.5"
  },
  "tags": [
    "webdriver",
    "selenium",
    "test",
    "automation",
    "browser",
    "javascript",
    "node"
  ],
  "keywords": [
    "webdriverio",
    "webdriver",
    "selenium",
    "appium",
    "saucelabs",
    "sauce",
    "labs",
    "mocha",
    "nodeUnit",
    "buster",
    "phantomjs",
    "chai",
    "vows",
    "jasmine",
    "assert",
    "cucumber",
    "testingbot"
  ],
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rm -rf ./build ./coverage",
    "compile": "babel src/ -d build/",
    "lint": "eslint src test",
    "postversion": "git tag -d $(git tag -l)",
    "release": "npm run release:patch",
    "release:patch": "np patch --no-cleanup",
    "release:minor": "np minor --no-cleanup",
    "release:major": "np major --no-cleanup",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "npm run compile -- --watch"
  },
  "dependencies": {
    "wdio-cli": "^0.0.1",
    "wdio-config": "^0.1.0",
    "wdio-logger": "^0.1.0",
    "webdriver": "^0.0.4"
  }
}
